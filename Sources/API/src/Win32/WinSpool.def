(*
 * Access rights for print servers
 *)

CONST SERVER_ALL_ACCESS = STANDARD_RIGHTS_REQUIRED +
                          SERVER_ACCESS_ADMINISTER +
                          SERVER_ACCESS_ENUMERATE;

