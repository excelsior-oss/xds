                           PRINTER_ACCESS_ADMINISTER +
                           PRINTER_ACCESS_USE;

      PRINTER_READ       = STANDARD_RIGHTS_READ +
                           PRINTER_ACCESS_USE;

      PRINTER_WRITE      = STANDARD_RIGHTS_WRITE +
                           PRINTER_ACCESS_USE;

      PRINTER_EXECUTE    = STANDARD_RIGHTS_EXECUTE +
                           PRINTER_ACCESS_USE;

